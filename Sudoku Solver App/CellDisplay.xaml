<UserControl x:Class="SudokuSolver.CellDislay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SudokuSolver"
             mc:Ignorable="d" 
             d:DesignHeight="25" d:DesignWidth="25">
    <Grid Width="25" Height="25">
        <TextBlock x:Name="CellText" Foreground="White" TextWrapping="Wrap" TextAlignment="Center" KeyDown="CellText_KeyDown" />
        
        
    </Grid>
</UserControl>
